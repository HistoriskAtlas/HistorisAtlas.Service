    name="SRW" />
   <endpoint address="http://webservice.bibliotek.dk/soeg" binding="basicHttpBinding"
    bindingConfiguration="Explain-SoapBinding" contract="ServiceReferenceBiblio.ExplainPort"
    name="ExplainSOAP" />
  </client>
 </system.serviceModel>
